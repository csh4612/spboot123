<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean id="brand" class="test.Brand">
<property name="brandName" value="자바"></property>
</bean>

<bean id="coffee" class="test.Coffee">
<property name="name" value="아메"></property>
<property name="price" value="5000"></property>
<property name="brand" ref="brand"></property>
</bean>
</beans>
